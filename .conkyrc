border_width 1
cpu_avg_samples 2
default_color gray
default_outline_color ${color #8B1A1A}
default_shade_color white
draw_borders yes
draw_graph_borders yes
draw_outline no
draw_shades no
double_buffer yes
use_xft yes
xftfont DejaVu Sans Mono:size=12
gap_x 4
gap_y 3
net_avg_samples 2
no_buffers yes
out_to_console no
out_to_stderr no
extra_newline no
own_window yes
own_window_type desktop
own_window_argb_visual yes
own_window_argb_value 210
own_window_transparent no
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
stippled_borders 0
update_interval 1
uppercase no
use_spacer none
show_graph_scale no
show_graph_range no
 

